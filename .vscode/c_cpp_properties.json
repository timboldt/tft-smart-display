{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "~/.arduino15/packages/rp2040/**",
                "~/Arduino/libraries/**"
            ],
            "defines": [
                "SERIALUSB_PID=0xf00a",
                "USBD_MAX_POWER_MA=250",
                "F_CPU=133000000L",
                "ARDUINO=10607",
                "ARDUINO_RASPBERRY_PI_PICO_W",
                "BOARD_NAME=RASPBERRY_PI_PICO_W",
                "ARDUINO_ARCH_RP2040"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64"
        }
    ],
    "version": 4
}